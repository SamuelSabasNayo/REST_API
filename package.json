{
  "name": "pet-industry-backend-project",
  "version": "1.0.0",
  "description": "pet-industry-backend-project",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production npx sequelize-cli db:migrate && cross-env NODE_ENV=production babel-node ./src/index.js",
    "createtables": "cross-env NODE_ENV=development npx sequelize db:migrate",
    "inserttables": "cross-env NODE_ENV=development npx sequelize db:seed:all",
    "deletetables": "cross-env NODE_ENV=development npx sequelize db:migrate:undo:all",
    "kill": "Taskkill /IM node.exe /F",
    "lint": "eslint ./src",
    "client-install": "cd client && npm install",
    "client-build": "cd client && npm run build",
    "client-start": "cd ../ && cd client && npm start",
    "server": "nodemon --exec babel-node ./src/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client-start\""
  },
  "keywords": "PET INDUSTRY",
  "author": "samuel",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/node": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/register": "^7.10.5",
    "@babel/runtime": "^7.11.2",
    "@hapi/joi": "^17.1.1",
    "babel-core": "^6.26.3",
    "babel-install": "2.1.0",
    "babel-loader": "^8.1.0",
    "babel-node": "0.0.1-security",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bcrypt": "^3.0.6",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.1",
    "nodemailer": "^6.4.11",
    "nodemailer-express-handlebars": "^4.0.0",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "path": "^0.12.7",
    "pg": "^7.18.2",
    "pg-hstore": "^2.3.3",
    "sendgrid": "^5.2.3",
    "sequelize": "^5.22.3",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "nodemon": "^2.0.4",
    "sequelize-cli": "^5.5.1"
  }
}